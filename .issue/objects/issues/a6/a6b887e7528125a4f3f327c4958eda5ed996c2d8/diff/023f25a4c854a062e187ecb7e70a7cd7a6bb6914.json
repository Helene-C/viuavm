[{"action": "open", "author": {"author.email": "marejm@ozro.pw", "author.name": "Marek Marecki"}, "timestamp": 1499194529.05986}, {"action": "set-message", "params": {"text": "Implement traps and interrupts\n\nViua should provide a way to interrupt processes mid-run if you know their PID.\nInterrupts would be run on a new stack in the process they interrupt if they are trapped, or\ncrash the process if they are not trapped.\n\nQuestion: should untrapped interrupts skip watchdog, or should watchdog handle interrupts?"}, "author": {"author.email": "marejm@ozro.pw", "author.name": "Marek Marecki"}, "timestamp": 1499194529.05988}, {"action": "push-tags", "params": {"tags": []}, "author": {"author.email": "marejm@ozro.pw", "author.name": "Marek Marecki"}, "timestamp": 1499194529.059886}, {"action": "push-milestones", "params": {"milestones": []}, "author": {"author.email": "marejm@ozro.pw", "author.name": "Marek Marecki"}, "timestamp": 1499194529.05989}, {"action": "push-tags", "params": {"tags": ["viuavm"]}, "author": {"author.email": "marejm@ozro.pw", "author.name": "Marek Marecki"}, "timestamp": 1499194529.060166}, {"action": "set-project-tag", "params": {"tag": "viuavm"}, "author": {"author.email": "marejm@ozro.pw", "author.name": "Marek Marecki"}, "timestamp": 1499194529.060173}, {"action": "set-project-name", "params": {"name": "Viua Virtual Machine"}, "author": {"author.email": "marejm@ozro.pw", "author.name": "Marek Marecki"}, "timestamp": 1499194529.060181}, {"action": "parameter-set", "params": {"key": "github_issue_no", "value": "180"}, "author": {"author.email": "marejm@ozro.pw", "author.name": "Marek Marecki"}, "timestamp": 1499194529.060288}]